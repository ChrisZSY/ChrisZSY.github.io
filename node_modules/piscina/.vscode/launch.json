  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Piscina Stream-In Example",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "cwd": "${workspaceFolder}/examples/stream-in",
      "env": {
        "NOTARE_HZ": "20"
      },
      "runtimeArgs": ["-r", "notare"],
      "program": "${workspaceFolder}/examples/stream-in/index.js"
    }
  ]
}
